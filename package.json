{
  "name": "beat-saber-song-info",
  "version": "2.0.0",
  "description": "This is a Discord bot to allow users to get information about Beat Saber songs hosted on the Beat Saber website.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "dev": "nodemon --watch src --exec \"ts-node\" src/index.ts",
    "deploy": "ts-node src/deploy-commands.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FranciscoRibeiro03/beat-saber-song-info.git"
  },
  "author": "rui2015",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/FranciscoRibeiro03/beat-saber-song-info/issues"
  },
  "homepage": "https://github.com/FranciscoRibeiro03/beat-saber-song-info",
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "beatsaver-api": "^2.1.0",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
